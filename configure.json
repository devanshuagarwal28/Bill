{
  "root": {
    "$CUST_PKG_DIR": "$PROJ_DIR$/.customPkgs",
    "$UTILS_DIR": "$CUST_PKG_DIR$/utils"
  },
  "dyn": {
    "$DYN_DIR": "$UTILS_DIR$/dyn",
    "$NODE_DYN_FILE": "dyn.js",
    "node": "$DYN_DIR$/dyn.jst"
  },
  "src": {
    "$SRC_DIR": "$PROJ_DIR$/src",
    "$PY_SRC_DIR": "$SRC_DIR$/py",
    "api": {
      "xlsxToJson": {
        "bin": "$PYBIN$",
        "cwd": "$PY_SRC_DIR$",
        "filePath": "$PY_SRC_DIR$/xlsxToJson.py",
        "argv0": "xlsxToJson.py",
        "argv": {
          "filePath": "",
          "jsonFilePath": ""
        }
      }
    }
  },
  "data": {
    "$DATA_DIR": "$PROJ_DIR$/data",
    "$LOG_DIR": "$DATA_DIR$/logs",
    "$SESSION_DIR": "$DATA_DIR$/sessions",
    "$DB_DIR": "$DATA_DIR$/db",
    "$TEMP_DIR": "$DATA_DIR$/temp",
    "db": {
      "sqlite3": {
        "$SQLITE3_NODE_PATH": "$DB_DIR$/sqlite3"
      }
    },
    "database": {
      "db": {
        "session": {
          "eng": "sqlite3",
          "dbFile": "$DB_DIR$/sqlite3/session.db"
        }
      }
    },
    "repo": {
      "$REPO_DIR": "$DATA_DIR$/repo",
      "session": {
        "$SESSION_REPO_DIR": "$REPO_DIR$/session"
      }
    }
  },
  "server": {
    "$SERVER_DIR": "$PROJ_DIR$/server",
    "type": "node",
    "st": "REST",
    "host": {
      "address": "0.0.0.0",
      "port": 8686
    },
    "types": {
      "node": {
        
      }
    }
  },
  "client": {
    
    "$CLIENT_DIR": "$PROJ_DIR$/client",
    "type": "web",
    "types": {
      "web": {
        "server": "file",
        "host":{
          "address": "0.0.0.0",
          "port": 8585
        },
        "url": "http://127.0.0.1:8585",
        "servers": {
          "file": {
            "staticFiles": "$CLIENT_DIR$/web/server/file/static/",
            "provider": "node",
            "urlMapping": {
              "home": "index.html"
            },
            "providers": {
              "node": {
                
              }
            }
          },
          "react": {}
        }
      }
    }
  }
}